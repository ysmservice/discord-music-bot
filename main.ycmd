NMethod on_ready
invoke  print "Logged_in"
EMethod
NMethod on_message
set  eval "True" true
set  eval "False" false
set  eval "None" none
set  eval "self.getvar('message').content" content
set content startswith "y!" iscmd
if iscmd == true
set content replace "y!","" cmdt
set cmdt split  cmdarr
set cmdarr pop 0 cmd
invoke  exec "self.var['cmdfile']=ClassData('cmds/'+self.getvar('cmd')+'.ycmd')"
invoke ycmd cmd_file cmdfile
Eif
EMethod
